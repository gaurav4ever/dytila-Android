                <!--</RelativeLayout>-->
            </LinearLayout>

            </android.support.v4.widget.NestedScrollView>


        </LinearLayout>

    </LinearLayout>
    <!--android:layout_gravity="start"-->

    <!-- Navigation Bar-->
    <RelativeLayout
        android:layout_width="250dp"
        android:layout_height="match_parent"
        android:id="@+id/navigation_view"
        android:background="#fff"
        android:layout_gravity="start"
        >

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/navHeader">
            <de.hdodenhof.circleimageview.CircleImageView
                xmlns:app="http://schemas.android.com/apk/res-auto"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:padding="4dp"
                android:src="@drawable/avatar"
                android:layout_marginTop="15dp"
                android:layout_marginLeft="4dp"
                app:civ_border_width="1dp"
                app:civ_border_color="#000000"
                android:id="@+id/userImg"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Dytila Nutrition"
                android:textSize="12dp"
                android:id="@+id/username"
                android:textColor="#000"
                android:layout_marginTop="35dp"
                android:layout_toRightOf="@+id/userImg"
                android:layout_marginLeft="10dp"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="dytilanutrition@gmail.com"
                android:textSize="10dp"
                android:id="@+id/user_email"
                android:textColor="#000"
                android:layout_below="@+id/username"
                android:layout_toRightOf="@+id/userImg"
                android:layout_marginTop="2dp"
                android:layout_marginLeft="10dp"
                />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_marginTop="15dp"
            android:layout_below="@+id/navHeader"
            android:id="@+id/divider1"
            android:background="@android:color/darker_gray"/>

        <android.support.v4.widget.NestedScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/divider1"
            android:layout_above="@+id/businessLayout">
            <include
                layout="@layout/navigation_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/options" />
        </android.support.v4.widget.NestedScrollView>


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="10dp"
            android:id="@+id/businessLayout"
            android:layout_above="@+id/divider2"
            android:background="#ab0303">
            <ImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:src="@drawable/business_icon1"
                android:layout_centerVertical="true"
                android:id="@+id/bimg"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Try Dytila Business"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/bimg"
                android:layout_marginLeft="20dp"
                android:textColor="#fff"
                android:textSize="12sp"/>
        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_marginBottom="10dp"
            android:layout_above="@+id/footerText1"
            android:id="@+id/divider2"
            android:background="@android:color/darker_gray"/>
        <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_below="@+id/divider2"
            android:src="@drawable/logo_main2"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="10dp"
            android:id="@+id/logo"/>
        <TextView
