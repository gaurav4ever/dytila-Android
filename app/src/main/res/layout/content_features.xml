<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:ads="http://schemas.android.com/apk/res-auto"
    android:id="@+id/drawer_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    android:background="#03032aab"
    android:layout_marginTop="0dp"
    tools:context="com.Dytila.gauravpc.dytilasp1.features"
    tools:showIn="@layout/activity_features">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

    <include
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:id="@+id/toolbar"
        layout="@layout/toolbar_layout"
        />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            >

            <android.support.v4.widget.NestedScrollView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                app:layout_behavior="@string/appbar_scrolling_view_behavior"
                >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="110dp"
                    android:background="@drawable/back3"
                    android:padding="10dp">
                    <android.support.v7.widget.CardView
                        xmlns:card_view="http://schemas.android.com/apk/res-auto"
                        xmlns:android="http://schemas.android.com/apk/res/android"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        card_view:contentPaddingTop="15dp"
                        card_view:contentPaddingBottom="15dp"
                        card_view:contentPaddingLeft="5dp"
                        card_view:contentPaddingRight="5dp"
                        card_view:cardElevation="2dp"
                        card_view:cardCornerRadius="5dp"
                        android:id="@+id/info0"
                        >
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:weightSum="5">
                            <RelativeLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:id="@+id/il1">
                                <ImageView
                                    android:layout_width="45dp"
                                    android:layout_height="30dp"
                                    android:layout_centerHorizontal="true"
                                    android:src="@drawable/askdytila_icon"
                                    android:id="@+id/askDytilaImg"
                                    />
                                <TextView
                                    android:layout_width="75dp"
                                    android:layout_height="wrap_content"
                                    android:layout_below="@+id/askDytilaImg"
                                    android:gravity="center"
                                    android:layout_centerHorizontal="true"
                                    android:layout_marginTop="3dp"
                                    android:textSize="10sp"
                                    android:textColor="#000"
                                    android:text="Ask Dytila"/>
                            </RelativeLayout>
                            <RelativeLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:layout_toRightOf="@id/il1"
                                android:id="@+id/il2">
                                <ImageView
                                    android:layout_width="45dp"
                                    android:layout_height="30dp"
                                    android:src="@drawable/slabs_icon"
                                    android:layout_centerHorizontal="true"
                                    android:id="@+id/slabsImg"
                                    />
                                <TextView
                                    android:layout_width="75dp"
                                    android:layout_height="wrap_content"
                                    android:layout_below="@+id/slabsImg"
                                    android:gravity="center"
                                    android:layout_centerHorizontal="true"
                                    android:textSize="10sp"
                                    android:layout_marginTop="3dp"
                                    android:textColor="#000"
                                    android:text="SLabs"/>
                            </RelativeLayout>
                            <RelativeLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:id="@+id/il3">
                                <ImageView
                                    android:layout_width="45dp"
                                    android:layout_height="30dp"
                                    android:src="@drawable/calculator"
                                    android:id="@+id/calImg"
                                    android:layout_centerHorizontal="true"
                                    />
                                <TextView
                                    android:layout_width="55dp"
                                    android:layout_height="wrap_content"
                                    android:layout_below="@+id/calImg"
                                    android:gravity="center"
                                    android:textSize="10sp"
                                    android:layout_marginTop="3dp"
                                    android:layout_centerHorizontal="true"
                                    android:textColor="#000"
                                    android:text="Calculators"/>
                            </RelativeLayout>
                            <RelativeLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:id="@+id/il7">
                                <ImageView
                                    android:layout_width="45dp"
                                    android:layout_height="30dp"
                                    android:src="@drawable/heart"
                                    android:layout_centerHorizontal="true"
                                    android:id="@+id/myKitchen"
                                    />
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_below="@+id/myKitchen"
                                    android:gravity="center"
                                    android:textSize="10sp"
                                    android:layout_centerHorizontal="true"
                                    android:layout_marginTop="3dp"
                                    android:textColor="#000"
                                    android:text="My Kitchen"/>
                            </RelativeLayout>
                            <RelativeLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:id="@+id/il8">
                                <ImageView
                                    android:layout_width="45dp"
                                    android:layout_height="31dp"
                                    android:src="@drawable/more_options_icon"
                                    android:layout_centerHorizontal="true"
                                    android:id="@+id/moreOptions"
                                    />
                                <TextView
                                    android:layout_width="75dp"
                                    android:layout_height="wrap_content"
                                    android:layout_below="@+id/moreOptions"
                                    android:gravity="center"
                                    android:textSize="10sp"
                                    android:layout_centerHorizontal="true"
                                    android:layout_marginTop="3dp"
                                    android:textColor="#000"
                                    android:text="More..."/>
                            </RelativeLayout>
                        </LinearLayout>

                    </android.support.v7.widget.CardView>
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="20dp">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Welcome First Last"
                        android:gravity="center"
                        android:id="@+id/welcomeText"
                        android:textSize="22sp"
                        android:textColor="#000"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Ready For Your Meal?"
                        android:gravity="center"
                        android:layout_marginTop="15dp"
                        android:layout_below="@+id/welcomeText"
                        android:id="@+id/welcomeText2"
                        android:textSize="18sp"
                        />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="5dp"
                    >
                    <android.support.v7.widget.CardView
                        xmlns:card_view="http://schemas.android.com/apk/res-auto"
                        xmlns:android="http://schemas.android.com/apk/res/android"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="5dp"
                        android:layout_marginBottom="5dp"
                        card_view:contentPaddingTop="20dp"
                        card_view:contentPaddingBottom="20dp"
                        card_view:cardElevation="1dp"
                        card_view:cardCornerRadius="5dp"
                        android:id="@+id/info1"
                        >
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            >
                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:id="@+id/layer1">
                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:weightSum="4">
                                    <RelativeLayout
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:id="@+id/veg">
                                        <ImageView
                                            android:layout_width="match_parent"
                                            android:layout_height="35dp"
                                            android:src="@drawable/veg_icon"
                                            android:id="@+id/veg_icon"
                                            />
                                        <TextView
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_below="@+id/veg_icon"
                                            android:gravity="center"
                                            android:textSize="10sp"
                                            android:layout_marginTop="3dp"
                                            android:textColor="#000"
                                            android:text="Veg Meals"/>
                                    </RelativeLayout>
                                    <RelativeLayout
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:id="@+id/nonveg">
                                        <ImageView
                                            android:layout_width="match_parent"
                                            android:layout_height="35dp"
                                            android:src="@drawable/nonveg_icon"
                                            android:id="@+id/nonveg_icon"
                                            />
                                        <TextView
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_below="@+id/nonveg_icon"
                                            android:gravity="center"
                                            android:textSize="10sp"
                                            android:layout_marginTop="3dp"
                                            android:textColor="#000"
                                            android:text="NonVeg Meals"/>
                                    </RelativeLayout>
                                    <RelativeLayout
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:id="@+id/egg">
                                        <ImageView
                                            android:layout_width="match_parent"
                                            android:layout_height="35dp"
                                            android:src="@drawable/egg_icon"
                                            android:id="@+id/egg_icon"
                                            />
                                        <TextView
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_below="@+id/egg_icon"
                                            android:gravity="center"
                                            android:textSize="10sp"
                                            android:layout_marginTop="3dp"
                                            android:textColor="#000"
                                            android:text="Egg Meals"/>
                                    </RelativeLayout>
                                    <RelativeLayout
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:id="@+id/salad">
                                        <ImageView
                                            android:layout_width="match_parent"
                                            android:layout_height="35dp"
                                            android:src="@drawable/salads_icon"
                                            android:id="@+id/salad_icon"
                                            />
                                        <TextView
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_below="@+id/salad_icon"
                                            android:gravity="center"
                                            android:textSize="10sp"
                                            android:layout_marginTop="3dp"
                                            android:textColor="#000"
                                            android:text="Salads"/>
                                    </RelativeLayout>
                                </LinearLayout>

                            </RelativeLayout>
                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_below="@+id/layer1"
                                android:layout_marginTop="25dp"
                                android:id="@+id/layer2">
                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:weightSum="4">
                                    <RelativeLayout
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:id="@+id/juices">
                                        <ImageView
                                            android:layout_width="match_parent"
                                            android:layout_height="35dp"
                                            android:src="@drawable/juices_icon"
                                            android:id="@+id/juices_icon"
                                            />
                                        <TextView
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_below="@+id/juices_icon"
                                            android:gravity="center"
                                            android:textSize="10sp"
                                            android:layout_marginTop="3dp"
                                            android:textColor="#000"
                                            android:text="Juices"/>
                                    </RelativeLayout>
                                    <RelativeLayout
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:id="@+id/shakes">
                                        <ImageView
                                            android:layout_width="match_parent"
                                            android:layout_height="35dp"
                                            android:src="@drawable/shakes_icon"
                                            android:id="@+id/shakes_icon"
                                            />
                                        <TextView
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_below="@+id/shakes_icon"
                                            android:gravity="center"
                                            android:textSize="10sp"
                                            android:layout_marginTop="3dp"
                                            android:textColor="#000"
                                            android:text="Shakes"/>
                                    </RelativeLayout>
                                    <RelativeLayout
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:id="@+id/cheat">
                                        <ImageView
                                            android:layout_width="match_parent"
                                            android:layout_height="35dp"
                                            android:src="@drawable/cheat_icon"
                                            android:id="@+id/cheat_icon"
                                            />
                                        <TextView
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_below="@+id/cheat_icon"
                                            android:gravity="center"
                                            android:textSize="10sp"
                                            android:layout_marginTop="3dp"
                                            android:textColor="#000"
                                            android:text="Cheat Stash"/>
                                    </RelativeLayout>
                                    <RelativeLayout
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:id="@+id/custom">
                                        <ImageView
                                            android:layout_width="match_parent"
                                            android:layout_height="35dp"
                                            android:src="@drawable/cutsom_icon3"
                                            android:id="@+id/custom_icon"
                                            />
                                        <TextView
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_below="@+id/custom_icon"
                                            android:gravity="center"
                                            android:textSize="10sp"
                                            android:layout_marginTop="3dp"
                                            android:textColor="#000"
                                            android:text="Custom"/>
                                    </RelativeLayout>
                                </LinearLayout>

                            </RelativeLayout>
                        </RelativeLayout>


                    </android.support.v7.widget.CardView>
                </RelativeLayout>

                <android.support.v7.widget.CardView
                    xmlns:card_view="http://schemas.android.com/apk/res-auto"
                    xmlns:android="http://schemas.android.com/apk/res/android"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    card_view:cardCornerRadius="0dp"
                    card_view:cardElevation="0.5dp"
                    card_view:cardBackgroundColor="#fff"
                    android:id="@+id/info4"
                    >
                    <HorizontalScrollView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:scrollbars="none">
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:padding="10dp"
                            >
                            <ImageView
                                android:layout_width="210dp"
                                android:layout_height="105dp"
                                android:id="@+id/img0"
                                android:src="@drawable/default_img2"/>
                            <ImageView
                                android:layout_width="210dp"
                                android:layout_height="105dp"
                                android:layout_toRightOf="@+id/img0"
                                android:id="@+id/img1"
                                android:layout_marginLeft="10dp"
                                android:src="@drawable/default_img2"/>
                            <ImageView
                                android:layout_width="210dp"
                                android:layout_height="105dp"
                                android:layout_toRightOf="@+id/img1"
                                android:id="@+id/img2"
                                android:layout_marginLeft="10dp"
                                android:src="@drawable/default_img2"/>
                            <ImageView
                                android:layout_width="210dp"
                                android:layout_height="105dp"
                                android:layout_toRightOf="@+id/img2"
                                android:id="@+id/img3"
                                android:layout_marginLeft="10dp"
                                android:src="@drawable/default_img2"/>
                            <ImageView
                                android:layout_width="210dp"
                                android:layout_height="105dp"
                                android:layout_marginLeft="10dp"
                                android:layout_toRightOf="@+id/img3"
                                android:id="@+id/img4"
                                android:src="@drawable/default_img2"/>
                            <ImageView
                                android:layout_width="210dp"
                                android:layout_height="105dp"
                                android:layout_toRightOf="@+id/img4"
                                android:id="@+id/img5"
                                android:layout_marginLeft="10dp"
                                android:src="@drawable/default_img2"/>
                            <ImageView
                                android:layout_width="210dp"
                                android:layout_height="105dp"
                                android:layout_toRightOf="@+id/img5"
                                android:id="@+id/img6"
                                android:layout_marginLeft="10dp"
                                android:src="@drawable/default_img2"/>
                            <ImageView
                                android:layout_width="210dp"
                                android:layout_height="105dp"
                                android:layout_toRightOf="@+id/img6"
                                android:id="@+id/img7"
                                android:layout_marginLeft="10dp"
                                android:src="@drawable/default_img2"/>
                            <ImageView
                                android:layout_width="210dp"
                                android:layout_height="105dp"
                                android:layout_toRightOf="@+id/img7"
                                android:id="@+id/img8"
                                android:layout_marginLeft="10dp"
                                android:src="@drawable/default_img2"/>
                        </RelativeLayout>
                    </HorizontalScrollView>
                </android.support.v7.widget.CardView>


                <!-- Google AdMobs-->

                <!--<RelativeLayout-->
                    <!--android:layout_width="match_parent"-->
                    <!--android:layout_height="wrap_content"-->
                    <!--android:id="@+id/adsLayout">-->
