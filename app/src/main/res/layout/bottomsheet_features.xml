                <RelativeLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:id="@+id/kitchens">
                    <ImageView
                        android:layout_width="55dp"
                        android:layout_height="55dp"
                        android:src="@drawable/kitchen_icon"
                        android:id="@+id/kitchens_icon"
                        android:layout_centerHorizontal="true"
                        />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/kitchens_icon"
                        android:gravity="center"
                        android:textSize="10sp"
                        android:layout_marginTop="5dp"
                        android:textColor="#000"
                        android:text="Kitchens"/>
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:id="@+id/feedback">
                    <ImageView
                        android:layout_width="55dp"
                        android:layout_centerHorizontal="true"
                        android:layout_height="55dp"
                        android:src="@drawable/feedback_icon"
                        android:id="@+id/feedback_icon"
                        />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/feedback_icon"
                        android:gravity="center"
                        android:textSize="10sp"
                        android:layout_marginTop="5dp"
                        android:textColor="#000"
                        android:text="Feedback"/>
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:id="@+id/facebook">
                    <ImageView
                        android:layout_width="55dp"
                        android:layout_height="55dp"
                        android:layout_centerHorizontal="true"
                        android:src="@drawable/facebook_icon"
                        android:id="@+id/facebook_icon"
                        />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/facebook_icon"
                        android:gravity="center"
                        android:textSize="10sp"
                        android:layout_marginTop="5dp"
                        android:textColor="#000"
                        android:text="Facebook"/>
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:id="@+id/instagram">
                    <ImageView
                        android:layout_width="55dp"
                        android:layout_height="55dp"
                        android:src="@drawable/instagram_icon"
                        android:layout_centerHorizontal="true"
                        android:id="@+id/instagram_icon"
                        />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/instagram_icon"
                        android:gravity="center"
                        android:textSize="10sp"
                        android:layout_marginTop="5dp"
                        android:textColor="#000"
                        android:text="Instagram"/>
                </RelativeLayout>
            </LinearLayout>

        </RelativeLayout>

</LinearLayout>